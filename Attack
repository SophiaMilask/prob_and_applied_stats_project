public class Attack {
    private int attackCost;
    private String type;
    private String name;
    private int damage;

    public Attack(int attackCost, String type, String name, int damage) {
        this.attackCost = attackCost;
        this.type = type;
        this.name = name;
        this.damage = damage;
    }

    public int getAttackCost() {
        return attackCost;
    }

    public String getType() {
        return type;
    }

    public String getName() {
        return name;
    }

    public int getDamage() {
        return damage;
    }

    public void setDamage(int damage) {
        this.damage = damage;
    }
}
